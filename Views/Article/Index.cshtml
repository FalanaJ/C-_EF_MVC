@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model ICollection<AS_Kol_1.Models.Article>

<h1>@ViewBag.Title</h1>

<p>Lista artykułów</p>

@if (Model != null)
{
	foreach (var a in Model)
	{
		<hr class="rounded">
		<p>
			<p>@a.Title</p>
			<p>@a.Lead</p>
			<p>@a.CreationDate</p>
		</p>
		<hr class="rounded">
	}
}



